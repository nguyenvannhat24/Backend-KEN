# Database
MONGO_URI=mongodb+srv://<username>:<password>@<cluster>.mongodb.net/<database>

# JWT Secrets
JWT_SECRET=your_jwt_secret_here
JWT_REFRESH_SECRET=your_jwt_refresh_secret_here

# Keycloak Configuration
KEYCLOAK_REALM=your_realm
KEYCLOAK_AUTH_URL=https://your-keycloak-server/auth
KEYCLOAK_JWKS_URI=https://your-keycloak-server/auth/realms/your_realm/protocol/openid-connect/certs
KEYCLOAK_RESOURCE=your_client_id
KEYCLOAK_PUBLIC_CLIENT=true
KEYCLOAK_SSL_REQUIRED=external
KEYCLOAK_CONFIDENTIAL_PORT=0

# CORS
CORS_ORIGIN=http://localhost:3000

# Server
PORT=3005
NODE_ENV=development
